<!-- Code in this file is available to the entire site -->
<!DOCTYPE html>
<html>
  <head>
    <title>Product-Ok</title>
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
      integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
      crossorigin="anonymous"> 
      <%= javascript_include_tag '//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js', 'data-turbolinks-track' => true %>
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %> 
      <%= javascript_include_tag 'application','data-turbolinks-track' => true %>
      <%= csrf_meta_tags %>
    </script>
  </head>

  <body>
    <!-- navbar -->
    <%= render 'layouts/navigation' %> 
    <!-- validation error messages -->
    <%= render 'layouts/messages' %>
    <!-- views -->
    <div class='container-fluid'>
      <%= yield %>
    </div>
    <!-- Load in .js files dynamically when you need them: 
    <script>
      <%= javascript_include_tag 'application','data-turbolinks-track' => true %>
      <%= yield :javascript_includes %>
    </script>
    -->
  </body>
</html>
