.custom-well {
  margin-top: 20px;
  display: inline-block;
  border: 2px solid #7097af;
  background: #b7cddc;
  font-size: 3rem;
  color: #314652;
  padding: 15px;
  border-radius: 35px 35px
}

.increase-font-size {
  font-size: 3rem;
}

.increase-font-size > a{
  font-weight: bold;
  color: #465f6e;
}